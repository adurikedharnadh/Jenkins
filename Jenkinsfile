pipeline{
    agent any
    stages{
        stage('build'){
            steps{
                echo "This is a sample building block"
            }
            
        }
        stage('test'){
            steps{
                echo "This is testing the application"
            }
            
        }
        stage('deploy'){
            steps{
                echo "Here we are deploying the application "
            }
            
        }
    }
}
